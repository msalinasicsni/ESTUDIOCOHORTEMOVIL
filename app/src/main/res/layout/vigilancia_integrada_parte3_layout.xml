        android:background="@drawable/bg_textview_rectangulo"
        android:text="@string/label_condicion"
        android:layout_marginLeft="14dp"
        android:textAppearance="@style/LabelDiagnosticocreensEstilo"
        android:textStyle="bold"
        />
