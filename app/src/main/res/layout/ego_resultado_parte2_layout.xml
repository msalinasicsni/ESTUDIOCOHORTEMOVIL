<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/txtvExamenMicroscopicoEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:text="@string/label_examen_microscopico_ego"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/txtvResultadoEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="90dp"
        android:layout_toRightOf="@+id/txtvExamenMicroscopicoEGO"
        android:text="@string/label_resultado_malaria"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/txtvCelulasEpiteliales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="17dp"
        android:layout_below="@+id/txtvExamenMicroscopicoEGO"
        android:text="@string/label_celulas_epiteliales"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtCelulasEpiteliales"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/txtvResultadoEGO"
        android:layout_marginLeft="24dp"
        android:layout_toRightOf="@+id/txtvCelulasEpiteliales"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvLeucocitosEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvCelulasEpiteliales"
        android:text="@string/label_leucocitos"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtLeucocitosEGO"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/edtxtCelulasEpiteliales"
        android:layout_marginLeft="71dp"
        android:layout_toRightOf="@+id/txtvLeucocitosEGO"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvEritrocitosEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvLeucocitosEGO"
        android:text="@string/label_eritrocitos"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtEritrocitosEGO"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/edtxtLeucocitosEGO"
        android:layout_marginLeft="74dp"
        android:layout_toRightOf="@+id/txtvEritrocitosEGO"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvCilindros"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvEritrocitosEGO"
        android:text="@string/label_cilindros"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtCilindros"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/edtxtEritrocitosEGO"
        android:layout_marginLeft="84dp"
        android:layout_toRightOf="@+id/txtvCilindros"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvCristales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvCilindros"
        android:text="@string/label_cristales"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtCristales"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/edtxtCilindros"
        android:layout_marginLeft="87dp"
        android:layout_toRightOf="@+id/txtvCristales"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvHilosMucosos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvCristales"
        android:text="@string/label_hilos_mucosos"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtHilosMucosos"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/edtxtCristales"
        android:layout_marginLeft="43dp"
        android:layout_toRightOf="@+id/txtvHilosMucosos"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvBacteriasEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvHilosMucosos"
        android:text="@string/label_bacterias"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtBacteriasEGO"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/edtxtHilosMucosos"
        android:layout_marginLeft="82dp"
        android:layout_toRightOf="@+id/txtvBacteriasEGO"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvLevadurasEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvBacteriasEGO"
        android:text="@string/label_levaduras"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtLevadurasEGO"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/edtxtBacteriasEGO"
        android:layout_marginLeft="76dp"
        android:layout_toRightOf="@+id/txtvLevadurasEGO"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

    <TextView
        android:id="@+id/txtvObservacionesEGO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvLevadurasEGO"
        android:text="@string/label_observaciones_examenes"
        android:textStyle="bold"
        />

    <EditText
        android:id="@+id/edtxtObservacionesEGO"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/edtxtLevadurasEGO"
        android:layout_marginLeft="46dp"
        android:layout_toRightOf="@+id/txtvObservacionesEGO"
        android:inputType="textMultiLine"
        android:scrollbars="vertical"
        android:enabled="false"
        style="@style/EditTextEstilo"
        />

</RelativeLayout>