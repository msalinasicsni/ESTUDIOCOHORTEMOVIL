<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:focusable="true" android:focusableInTouchMode="true">

    <TextView
        android:id="@+id/txtvTituloExamenResultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:textAppearance="@style/TextoTituloEstilo"
        android:text="@string/title_examenes_resultados"
        />
    <Button
        android:id="@+id/btnActualizarResultados"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/txtvTituloExamenResultado"
        android:background="@drawable/bg_button_enviar_selector"
        android:text="@string/boton_actualizar_resultado"
        />
    <TextView
        android:id="@+id/txtvNumOrdenLaboratorio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/btnActualizarResultados"
        android:text="@string/label_orden_lab"
        android:textSize="22sp"
        android:textColor="@android:color/black"
        android:inputType="text"
        android:visibility="invisible"
        android:singleLine="true"
        android:maxLength="32"
        />
    <EditText
        android:id="@+id/edtxNumOrenLaboratorio"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_toRightOf="@+id/txtvNumOrdenLaboratorio"
        android:layout_marginTop="26dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="7dp"
        android:layout_below="@+id/btnActualizarResultados"
        android:paddingLeft="14dp"
        android:background="@drawable/bg_underline_edittext"
        android:textColor="@color/color_texto_deshabilitado"
        android:textSize="17sp"
        android:visibility="invisible"
        android:enabled="false"
        android:ems="10"
        />
   <!-- <Button
        android:id="@+id/btnBHCesultado"
