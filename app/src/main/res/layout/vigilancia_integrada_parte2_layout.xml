        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_toRightOf="@+id/txtvAntiMeningSi"
        android:layout_below="@+id/txtvPentavalenteNoDosis"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiMenigNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvPentavalenteNoDosis"
        android:layout_toRightOf="@+id/chkAntiMenigSi"
        android:text="@string/label_vacuna_no"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkAntiMenigNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_toRightOf="@+id/txtvAntiMenigNo"
        android:layout_below="@+id/txtvPentavalenteNoDosis"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiMenigNoSabe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvPentavalenteNoDosis"
        android:layout_toRightOf="@+id/chkAntiMenigNo"
        android:text="@string/label_vacuna_no_sabe"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkAntiMenigNoSabe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_toRightOf="@+id/txtvAntiMenigNoSabe"
        android:layout_below="@+id/txtvPentavalenteNoDosis"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvCualVacunaAntiMenig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/txtvVacunaAntiMening"
        android:text="@string/label_cual"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvConjugada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvVacunaAntiMening"
        android:layout_toRightOf="@+id/txtvCualVacunaAntiHib"
        android:text="@string/label_conjugada"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkConjugada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@+id/txtvConjugada"
        android:layout_below="@+id/txtvVacunaAntiMening"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvPolisacarida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/txtvVacunaAntiMening"
        android:layout_toRightOf="@+id/chkConjugada"
        android:text="@string/label_polisacarida"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkPolisacarida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@+id/txtvPolisacarida"
        android:layout_below="@+id/txtvVacunaAntiMening"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiMeningNoDosis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/txtvCualVacunaAntiMenig"
        android:text="@string/label_no_dosis"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <EditText
        android:id="@+id/edtxtNoDosisAntiMening"
