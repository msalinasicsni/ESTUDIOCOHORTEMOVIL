        android:layout_marginTop="18dp"
        android:layout_marginLeft="15dp"

        />

    <TextView
        android:id="@+id/txtvCitologiaFecal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_citologia_fecal"
        android:textAppearance="@style/LabelSintomasScreensEstilo"
        android:layout_alignBaseline="@+id/chkbSCitologiaFecalExamen"
        android:layout_alignBottom="@+id/chkbSCitologiaFecalExamen"
        android:layout_alignLeft="@+id/txtvEGH"
        android:layout_alignStart="@+id/txtvEGH" />

    <CheckBox
        android:id="@+id/chkbSCitologiaFecalExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chkbSEGHExamen"
