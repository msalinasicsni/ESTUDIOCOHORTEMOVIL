        android:layout_marginTop="40dp"
        android:background="@drawable/bg_button_verder_oscuro_selector"
        android:text="@string/boton_limpiar"
        android:textColor="@android:color/white"
        android:textSize="14dp" />

    <ImageButton
        android:id="@+id/ibtImprimir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/edtxtNoDosisAdministrada"
        android:layout_toRightOf="@+id/btnlimpiarFicha"
        android:src="@drawable/printer64"
        android:background="@null"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="40dp" />

    <ImageButton
        android:id="@+id/ibtFichaPDF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/edtxtNoDosisAdministrada"
        android:layout_toRightOf="@+id/ibtImprimir"
        android:src="@drawable/pdf64"
        android:background="@null"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="40dp" />-->
</RelativeLayout>