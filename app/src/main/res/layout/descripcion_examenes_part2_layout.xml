<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/txtvEGO"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="35dp"
        android:text="@string/label_ego"
        android:textAppearance="@style/LabelSintomasScreensEstilo"
        />

    <CheckBox
        android:id="@+id/chkbSEGOExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtvEGO"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="7dp"

        />

    <CheckBox
        android:id="@+id/chkbNEGOExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/chkbSEGOExamen"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="15dp"

        />

    <TextView
        android:id="@+id/txtvEGH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/label_egh"
        android:textAppearance="@style/LabelSintomasScreensEstilo"
        android:layout_above="@+id/chkbSCitologiaFecalExamen"
        android:layout_alignLeft="@+id/txtvEGO"
        android:layout_alignStart="@+id/txtvEGO" />

    <CheckBox
        android:id="@+id/chkbSEGHExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chkbSEGOExamen"
