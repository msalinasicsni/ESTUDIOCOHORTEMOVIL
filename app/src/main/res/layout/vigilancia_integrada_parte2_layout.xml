<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent">
    <TextView
        android:id="@+id/txtvAntecedentesVacuna"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="@string/label_antecedentes_vacunacion"
        android:textStyle="bold"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvAntecedentesVacunaTarjeta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/txtvAntecedentesVacuna"
        android:text="@string/label_antecedentes_vacunacion_tarjeta"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvSI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvAntecedentesVacuna"
        android:layout_toRightOf="@+id/txtvAntecedentesVacunaTarjeta"
        android:text="@string/label_vacuna_si"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkTarjetaVacunaSi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/txtvSI"
        android:layout_below="@+id/txtvAntecedentesVacuna"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@+id/txtvAntecedentesVacuna"
        android:layout_toRightOf="@+id/chkTarjetaVacunaSi"
        android:text="@string/label_vacuna_no"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkTarjetaVacunaNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_toRightOf="@+id/txtvNo"
        android:layout_below="@+id/txtvAntecedentesVacuna"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_gravity="center"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:background="#FAFAFA" />
    <TextView
        android:id="@+id/txtvVacuna"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:text="@string/label_vacuna"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvAntiHib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="5dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:layout_toRightOf="@+id/txtvVacuna"
        android:text="@string/label_anti_hib"
        android:textStyle="bold"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvAplicada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:layout_toRightOf="@+id/txtvAntiHib"
        android:text="@string/label_aplicada"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvAntiHibSi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:layout_toRightOf="@+id/txtvAplicada"
        android:text="@string/label_vacuna_si"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkAntiHibSi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/txtvAntiHibSi"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiHibNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:layout_toRightOf="@+id/chkAntiHibSi"
        android:text="@string/label_vacuna_no"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkAntiHibNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/txtvAntiHibNo"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiHibNoSabe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        android:layout_toRightOf="@+id/chkAntiHibNo"
        android:text="@string/label_vacuna_no_sabe"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkAntiHibNoSabe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/txtvAntiHibNoSabe"
        android:layout_below="@+id/txtvAntecedentesVacunaTarjeta"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvCualVacunaAntiHib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/txtvVacuna"
        android:text="@string/label_cual"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvPentavalente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvVacuna"
        android:layout_toRightOf="@+id/txtvCualVacunaAntiHib"
        android:text="@string/label_pentavalente"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkPentavalente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@+id/txtvPentavalente"
        android:layout_below="@+id/txtvAplicada"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvPentavalenteNoDosis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/txtvCualVacunaAntiHib"
        android:text="@string/label_no_dosis"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <EditText
        android:id="@+id/edtxtNoDosisPentavalente"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtvPentavalenteNoDosis"
        android:layout_below="@+id/txtvCualVacunaAntiHib"
        android:layout_marginLeft="10dp"
        android:enabled="false"
        android:maxLength="1"
        android:inputType="number"
        android:digits="12345"
        android:layout_marginTop="4dp"
        android:textColor="@android:color/black"
        />
    <TextView
        android:id="@+id/txtvFechaUltimdaDosisAntiHib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvCualVacunaAntiHib"
        android:layout_toRightOf="@+id/edtxtNoDosisPentavalente"
        android:text="@string/label_fecha_ultima_dosis"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <EditText
        android:id="@+id/dpFechaUltimaDosisAntiHib"
        android:layout_width="160dp"
        android:layout_height="40dp"
        android:inputType="date"
        android:editable="false"
        android:layout_toRightOf="@+id/txtvFechaUltimdaDosisAntiHib"
        android:layout_below="@+id/txtvCualVacunaAntiHib"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="8dp"
        android:ems="10"
        android:enabled="false"
        android:textCursorDrawable="@null"
        android:drawableRight="@drawable/calendar"
        style="@style/EditTextEstilo" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_gravity="center"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/txtvPentavalenteNoDosis"
        android:background="#FAFAFA" />

    <TextView
        android:id="@+id/txtvVacunaAntiMening"
