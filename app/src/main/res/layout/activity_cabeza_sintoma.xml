            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/txtvSCBSintoma"
            android:layout_below="@+id/txtvRGCCBSintoma"
            style="@style/customCheckboxStyle"
            android:layout_marginLeft="7dp"
