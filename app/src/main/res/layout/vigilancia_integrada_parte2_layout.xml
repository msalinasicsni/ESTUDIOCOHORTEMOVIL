    <CheckBox
        android:id="@+id/chkAntiNeumococicaNoSabe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_toRightOf="@+id/txtvAntiNeumococicaNoSabe"
        android:layout_below="@+id/txtvAntiMeningNoDosis"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvCualVacunaAntiNeumococica"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/txtvVacunaAntiNeumococica"
        android:text="@string/label_cual"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <TextView
        android:id="@+id/txtvHeptavalente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvVacunaAntiNeumococica"
        android:layout_toRightOf="@+id/txtvCualVacunaAntiNeumococica"
        android:text="@string/label_heptavalente"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkHeptavalente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@+id/txtvHeptavalente"
        android:layout_below="@+id/txtvVacunaAntiNeumococica"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtv23Polisacarida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/txtvVacunaAntiNeumococica"
        android:layout_toRightOf="@+id/chkHeptavalente"
        android:text="@string/label_23_polisacarida"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chk23Polisacarida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_toRightOf="@+id/txtv23Polisacarida"
        android:layout_below="@+id/txtvVacunaAntiNeumococica"
