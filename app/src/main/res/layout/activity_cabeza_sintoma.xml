<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent"
    tools:context=".sintomasactivities.CabezaSintomaActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent">

        <Button
            android:id="@+id/btnCabezaGSintomas"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:layout_marginTop="7dp"
            android:text="@string/boton_cabez_sintomas"
            android:layout_alignParentTop="true"
            android:drawableRight="@drawable/reducir"
            android:onClick="onClick_btnCabeza"
            />

        <TextView
            android:id="@+id/txtvPrinciplaCBSintoma"
            android:layout_width="420dp"
            android:layout_height="30dp"
            android:layout_below="@+id/btnCabezaGSintomas"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="14dp"
            android:gravity="center"
            android:background="@drawable/bg_textview_rectangulo"
            android:text="@string/label_sintoma"
            android:textAppearance="@style/LabelSintomasScreensEstilo"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/txtvSCBSintoma"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_toRightOf="@+id/txtvPrinciplaCBSintoma"
            android:layout_below="@+id/btnCabezaGSintomas"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:background="@drawable/bg_textview_rectangulo"
            android:text="@string/label_s"
            android:textAppearance="@style/LabelSintomasScreensEstilo"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/txtvNCBSintoma"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_toRightOf="@+id/txtvSCBSintoma"
            android:layout_below="@+id/btnCabezaGSintomas"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:background="@drawable/bg_textview_rectangulo"
            android:text="@string/label_n"
            android:textAppearance="@style/LabelSintomasScreensEstilo"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/txtvDCBSintoma"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_toRightOf="@+id/txtvNCBSintoma"
            android:layout_below="@+id/btnCabezaGSintomas"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="5dp"
            android:gravity="center"
            android:background="@drawable/bg_textview_rectangulo"
            android:text="@string/label_d"
            android:textAppearance="@style/LabelSintomasScreensEstilo"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/txtvCAFCBSintoma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtvPrinciplaCBSintoma"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="24dp"
            android:text="@string/label_cafalea"
            android:textAppearance="@style/LabelSintomasScreensEstilo"
            />

        <CheckBox
            android:id="@+id/chkbCAFSCBSintoma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/txtvPrinciplaCBSintoma"
            android:layout_below="@+id/txtvPrinciplaCBSintoma"
            style="@style/customCheckboxStyle"
            android:layout_marginLeft="7dp"
            android:onClick="onChkboxClickedCAFCB"
            />

        <CheckBox
            android:id="@+id/chkbCAFNCBSintoma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/txtvSCBSintoma"
