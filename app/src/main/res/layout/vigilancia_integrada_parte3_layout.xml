        android:id="@+id/chkEstornudosNs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtvCondicionNo"
        android:layout_below="@+id/txtvOtra"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="15dp"
        />





    <TextView
        android:id="@+id/txtvOtraManifestacionClinica"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtvEstornudos"
        android:layout_marginTop="26dp"
        android:text="@string/label_otra_manifestacion_clinica"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <EditText
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:id="@+id/edtxtOtraManifestacionClinica"
        android:layout_toRightOf="@+id/txtvOtraManifestacionClinica"
        android:layout_below="@+id/txtvEstornudos"
        android:layout_marginLeft="5dp"
        android:enabled="false"
        android:layout_marginTop="8dp"
        android:maxLength="64"
        android:textColor="@android:color/black"
        />
    <CheckBox
        android:id="@+id/chkOtraManifestacionClinicaSi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtvCondicion"
        android:layout_below="@+id/txtvEstornudos"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="15dp"
        />
    <CheckBox
        android:id="@+id/chkOtraManifestacionClinicaNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtvCondicionSi"
        android:layout_below="@+id/txtvEstornudos"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="15dp"
        />
    <CheckBox
        android:id="@+id/chkOtraManifestacionClinicaNs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/txtvCondicionNo"
        android:layout_below="@+id/txtvEstornudos"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="15dp"
        />
