    <CheckBox
        android:id="@+id/chkbSFactorReuExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/chkbSCitologiaFecalExamen"
        android:layout_toRightOf="@+id/txtvEGO"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="7dp"

        />

    <CheckBox
        android:id="@+id/chkbNFactorReuExamen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/chkbSCitologiaFecalExamen"
        android:layout_below="@+id/chkbNCitologiaFecalExamen"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="15dp"

        />

    <TextView
        android:id="@+id/txtvAlbumina"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
