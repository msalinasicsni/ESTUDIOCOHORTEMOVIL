    <CheckBox
        android:id="@+id/chkAntiNeumococicaSi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_toRightOf="@+id/txtvAntiNeumococicaSi"
        android:layout_below="@+id/txtvAntiMeningNoDosis"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiNeumococicaNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvAntiMeningNoDosis"
        android:layout_toRightOf="@+id/chkAntiNeumococicaSi"
        android:text="@string/label_vacuna_no"
        android:textAppearance="@style/TextViewEstiloSeguimiento" />
    <CheckBox
        android:id="@+id/chkAntiNeumococicaNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_toRightOf="@+id/txtvAntiNeumococicaNo"
        android:layout_below="@+id/txtvAntiMeningNoDosis"
        style="@style/customCheckboxStyle"
        android:layout_marginLeft="7dp"
        />
    <TextView
        android:id="@+id/txtvAntiNeumococicaNoSabe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/txtvAntiMeningNoDosis"
        android:layout_toRightOf="@+id/chkAntiNeumococicaNo"
        android:text="@string/label_vacuna_no_sabe"
